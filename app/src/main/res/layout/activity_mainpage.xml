<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainPage">

    <TextView
        android:id="@+id/username"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="60dp"
        android:text="Sveiki, username"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/username2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="80dp"
        android:layout_marginTop="100dp"
        android:text="Pasirinkite norimą muziejų:"
        android:textSize="21sp" />

    <Button
        android:id="@+id/button"
        android:layout_width="356dp"
        android:layout_height="63dp"
        android:layout_below="@+id/username2"
        android:layout_alignParentRight="false"
        android:layout_marginLeft="30dp"

        android:layout_marginTop="30dp"
        android:layout_marginRight="30dp"
        android:layout_marginBottom="20dp"
        android:background="#C5BFBF"
        android:text="Lietuvos Aviacijos muziejus" />

    <Button
        android:id="@+id/button9"
        android:layout_width="356dp"
        android:layout_height="63dp"
        android:layout_below="@+id/button"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="20dp"
        android:layout_marginRight="30dp"
        android:background="#C5BFBF"
        android:text="Vytauto Didžiojo karo muziejus" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="Atsijungti" />




</RelativeLayout>